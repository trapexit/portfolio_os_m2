\ SPLITEXEC - for splitting rate execution between odd and even frames
\

include? I_MixLeft dspp_map.j
include? DFID_SPLITEXEC function_ids.j

anew task-splitexec.ins

_GREEN
dspp{
ins{

DFID_SPLITEXEC dspp-function-id !
$" splitexec.dsp" ins-name !
dspp-echo off

0 $ FFFF 1 ALLOC.KNOB CountMask

10 ALLOC.TICKS

	I_FrameCount _A CountMask _A _AND
	ACCUME _TRA
	_BEQ: Done
_LABEL: Done
	
}ins
}dspp
